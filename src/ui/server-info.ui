<?xml version="1.0" encoding="UTF-8"?>

<interface>
  <requires lib="gtk+" version="4.0"/>

  <template class="GswUiServerInfo" parent="GtkWidget">
    <property name="hexpand">true</property>

    <child>
      <object class="GtkGrid">
        <property name="column-spacing">8</property>
        <property name="row-spacing">4</property>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Name:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <property name="wrap">true</property>
            <binding name="label">
              <closure type="gchararray" function="na_if_null">
                <lookup type="GswServerInfo" name="server-name">
                  <lookup type="GswQuerier" name="sinfo">
                    <lookup type="GswUiServerInfo" name="querier">
                      GswUiServerInfo
                    </lookup>
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Address:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="address_ctl">
            <layout>
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <binding name="label">
              <closure type="gchararray" function="na_if_null">
                <lookup type="GswServer" name="address">
                  <lookup type="GswQuerier" name="server">
                    <lookup type="GswUiServerInfo" name="querier">
                      GswUiServerInfo
                    </lookup>
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">2</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Location:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="location_ctl">
            <layout>
              <property name="column">1</property>
              <property name="row">2</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <binding name="label">
              <closure type="gchararray" function="na_if_null">
                <closure type="gchararray" function="format_location">
                  <lookup type="GswNetTransport" name="saddr">
                    <lookup type="GswQuerier" name="transport">
                      <lookup type="GswUiServerInfo" name="querier">
                        GswUiServerInfo
                      </lookup>
                    </lookup>
                  </lookup>
                </closure>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">3</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Game:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="game_ctl">
            <layout>
              <property name="column">1</property>
              <property name="row">3</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <binding name="label">
              <closure type="gchararray" function="na_if_null">
                <lookup type="GswServerInfo" name="game-name">
                  <lookup type="GswQuerier" name="sinfo">
                    <lookup type="GswUiServerInfo" name="querier">
                      GswUiServerInfo
                    </lookup>
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">4</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Mode:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="game_mode_ctl">
            <layout>
              <property name="column">1</property>
              <property name="row">4</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <binding name="label">
              <closure type="gchararray" function="na_if_null">
                <lookup type="GswServerInfo" name="game-mode">
                  <lookup type="GswQuerier" name="sinfo">
                    <lookup type="GswUiServerInfo" name="querier">
                      GswUiServerInfo
                    </lookup>
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">5</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Map:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="map_ctl">
            <layout>
              <property name="column">1</property>
              <property name="row">5</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <binding name="label">
              <closure type="gchararray" function="na_if_null">
                <lookup type="GswServerInfo" name="map">
                  <lookup type="GswQuerier" name="sinfo">
                    <lookup type="GswUiServerInfo" name="querier">
                      GswUiServerInfo
                    </lookup>
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">6</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Players:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="players_ctl">
            <layout>
              <property name="column">1</property>
              <property name="row">6</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <binding name="label">
              <closure type="gchararray" function="na_if_null">
                <closure type="gchararray" function="format_players">
                  <lookup type="GswServerInfo" name="num-players">
                    <lookup type="GswQuerier" name="sinfo">
                      <lookup type="GswUiServerInfo" name="querier">
                        GswUiServerInfo
                      </lookup>
                    </lookup>
                  </lookup>
                  <lookup type="GswServerInfo" name="max-players">
                    <lookup type="GswQuerier" name="sinfo">
                      <lookup type="GswUiServerInfo" name="querier">
                        GswUiServerInfo
                      </lookup>
                    </lookup>
                  </lookup>
                </closure>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">7</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Private:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="private_ctl">
            <layout>
              <property name="column">1</property>
              <property name="row">7</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <binding name="label">
              <closure type="gchararray" function="format_boolean">
                <lookup type="GswServerInfo" name="private">
                  <lookup type="GswQuerier" name="sinfo">
                    <lookup type="GswUiServerInfo" name="querier">
                      GswUiServerInfo
                    </lookup>
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">8</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Secure:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="secure_ctl">
            <layout>
              <property name="column">1</property>
              <property name="row">8</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <binding name="label">
              <closure type="gchararray" function="format_boolean">
                <lookup type="GswServerInfo" name="secure">
                  <lookup type="GswQuerier" name="sinfo">
                    <lookup type="GswUiServerInfo" name="querier">
                      GswUiServerInfo
                    </lookup>
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel">
            <layout>
              <property name="column">0</property>
              <property name="row">9</property>
            </layout>
            <property name="xalign">0</property>
            <property name="label" translatable="yes">Version:</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="version_ctl">
            <layout>
              <property name="column">1</property>
              <property name="row">9</property>
            </layout>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
            <property name="selectable">true</property>
            <binding name="label">
              <closure type="gchararray" function="na_if_null">
                <lookup type="GswServerInfo" name="game-version">
                  <lookup type="GswQuerier" name="sinfo">
                    <lookup type="GswUiServerInfo" name="querier">
                      GswUiServerInfo
                    </lookup>
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="error_ctl">
            <layout>
              <property name="column">0</property>
              <property name="row">10</property>
              <property name="column-span">2</property>
            </layout>
            <property name="name">error</property>
            <property name="css-classes">error</property>
            <binding name="label">
              <closure type="gchararray" function="format_error">
                <lookup type="GswQuerier" name="error">
                  <lookup type="GswUiServerInfo" name="querier">
                    GswUiServerInfo
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>

      </object>
    </child>

  </template>
</interface>
